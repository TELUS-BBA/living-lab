[Unit]
Description=scheduled network testing
After=network.target

[Service]
User=nanopi
ExecStart=/usr/bin/python3 -u /home/nanopi/schedule.py {{ test_host }} {{ iperf_port }} {{ management_host }} {{ management_port }} {{ info_path }} {{ ping_result_path }} {{ intense_result_path }} {{ test_time.stdout_lines[0] }}
RestartSec=3
Restart=always

[Install]
WantedBy=multi-user.target
